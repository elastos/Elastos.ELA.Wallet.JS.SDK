    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.5.2",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": "> 0.25%, not dead",
