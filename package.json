    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-file-content-replace": "^1.0.0",
    "rollup-plugin-multi-input": "^1.2.0",
