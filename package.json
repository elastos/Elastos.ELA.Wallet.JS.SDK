{
  "name": "@elastosfoundation/wallet-js-sdk",
  "version": "1.0.0",
  "description": "Elastos Wallet Javascript SDK",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "npm run clean && npm run types && rollup -c --environment prodbuild",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist/ typings/ generated/",
    "types": "npm run clean && tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "tsc:build": "rollup -c",
    "tsc:w": "tsc -w"
  },
  "exports": {
    "browser": {
      "import": "./dist/es/wallet.browser.js"
    }
  },
  "main": "./dist/wallet.js",
  "module": "./dist/es/wallet.js",
  "browser": {
    "./dist/wallet.js": "./dist/wallet.browser.js",
    "./dist/es/wallet.js": "./dist/es/wallet.browser.js"
  },
  "typings": "typings/",
  "dependencies": {
    "assert": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.0",
    "bip32": "^2.0.6",
    "bip39": "^3.0.4",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "btoa": "^1.2.1",
    "buffer": "^5.6.0",
    "create-hash": "^1.2.0",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.4",
    "elliptic": "^6.5.4",
    "events": "^3.3.0",
    "js-sha3": "^0.8.0",
